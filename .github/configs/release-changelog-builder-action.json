{
    "categories": [
        {
            "title": "## Features",
            "labels": ["enhancement"]
        },
        {
            "title": "## Fixes",
            "labels": ["bug"]
        },
        {
            "title": "## Others",
            "labels": []
        }
    ],
    "sort": {
        "order": "ASC",
        "on_property": "title"
    },
    "template": "${{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>",
    "pr_template": "- ${{TITLE}} (${{AUTHOR}})\n   - PR: #${{NUMBER}}",
    "empty_template": "- no changes",
    "max_pull_requests": 200,
    "max_back_track_time_days": 90,
    "tag_resolver": {
      "method": "semver",
      "filter": {
        "pattern": "api-(.+)",
        "flags": "gu"
      }
    },
    "base_branches": [
      "main"
    ]
}